{
  "private": true,
  "name": "tetra",
  "version": "0.0.1",
  "author": "sophia@tetratrust.com",
  "scripts": {
    "start": "node_modules/.bin/serverless offline start",
    "lint": "eslint -c ./.eslintrc.json . --ext .ts --fix",
    "test": "jest --config=jest.config.ts",
    "db:migrate": "node_modules/.bin/knex --knexfile knexfile.ts migrate:latest",
    "db:seed": "npm run db:migrate && TS_NODE_FILES=true node -r tsconfig-paths/register node_modules/.bin/knex --knexfile knexfile.ts seed:run",
    "db:rollback": "node_modules/.bin/knex --knexfile knexfile.ts migrate:rollback",
    "db:migrate:new": "node_modules/.bin/knex --knexfile knexfile.ts migrate:make -x ts --",
    "db:migrate:staging": "NODE_ENV=staging node -r ts-node/register database/migration-script.js",
    "db:migrate:production": "NODE_ENV=production node -r ts-node/register database/migration-script.js"
  },
  "dependencies": {
    "aws-sdk": "^2.1106.0",
    "knex": "^1.0.4",
    "objection": "^3.0.1",
    "pg": "^8.7.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.93",
    "@types/jest": "^27.4.1",
    "@types/node": "^14.18.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "serverless": "^3.10.2",
    "serverless-bundle": "^5.3.0",
    "serverless-offline": "^8.5.0",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tunnel-ssh": "^4.1.6",
    "typescript": "^4.6.3"
  }
}
